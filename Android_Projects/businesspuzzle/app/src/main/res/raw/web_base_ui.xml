<?xml version="1.0" encoding="utf-8"?>

<business_circiut
	name="web_base_ui"
	header="Web Base UI"
	subheader="">


	<input_group>

		<input
			name="node_js"
			caption="Node.js"/>

		<input
			name="angular"
			caption="Angular"/>

		<input
			name="react"
			caption="React"/>

		<input
			name="vue_js"
			caption="Vue.js"/>

	</input_group>



	<operators_group>

		<operator
			name="xor1"
			type="xor"
			in_one="node_js"
			in_two="angular"/>

		<operator
			name="xor2"
			type="xor"
			in_one="react"
			in_two="vue_js"/>

		<operator
			name="xor3"
			type="xor"
			in_one="xor1"
			in_two="xor2"/>

	</operators_group>



	<result_group>

		<result
			name="res1"
			caption=""
			in="xor3"
			text_success="Web Base UI/nFor devices without LCD"
			text_fail=""/>

	</result_group>


</business_circiut>